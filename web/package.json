    "vuetify": "^1.5.16"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.0.0",
    "vue-template-compiler": "^2.6.10",
    "vue-hot-reload-api": "^2.3.3",
    "@vue/test-utils": "^1.0.0-beta.29",
    "@babel/core": "^7.4.5",
    "babel-core": "^7.0.0-bridge.0",
    "@babel/preset-env": "^7.4.5",
    "babel-jest": "^24.8.0",
